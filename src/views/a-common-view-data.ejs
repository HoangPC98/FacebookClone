    FetchPostData = function (apiUrl, data) {
       // console.log('data passed', data)
       let post = {
           method: 'POST',
           headers: {
               "Content-type": "application/json; charset=UTF-8"
           },
           body: JSON.stringify(data)
       }
       fetch(apiUrl, post)
           .then(dataFetch => {
               console.log('POST SENDED', dataFetch)
               return dataFetch.json()
           })
           .then(data=>{
               console.log('FETCH POST OK', data) 
               return data     
           })
           .catch(error=>{
               console.log('ERROR FETCH:', error)
           })
   }

   // Fetch GetData
    FetchGetData = async function (apiUrl){
